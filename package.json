    "author": "",
    "license": "MIT",
    "dependencies": {
        "express": "^4.18.2",
        "mongoose": "^7.5.0",
        "bcryptjs": "^2.4.3",
        "jsonwebtoken": "^9.0.2",
        "cors": "^2.8.5",
        "helmet": "^7.0.0",
        "express-rate-limit": "^6.10.0",
        "express-validator": "^7.0.1",
        "dotenv": "^16.3.1",
        "stripe": "^13.5.0",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.9.4",
        "crypto": "^1.0.1",
        "compression": "^1.7.4",
        "morgan": "^1.10.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/node": "^20.5.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/cors": "^2.8.13",
        "@types/compression": "^1.7.2",
        "@types/morgan": "^1.9.4",
        "@types/multer": "^1.4.7",
        "@types/nodemailer": "^6.4.9",
        "@types/jest": "^29.5.4",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^6.4.0",
        "@typescript-eslint/parser": "^6.4.0",
        "eslint": "^8.47.0",
        "jest": "^29.6.2",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.1.6"
    }
