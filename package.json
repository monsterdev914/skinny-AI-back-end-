{
    "name": "skinny-ai-backend",
    "version": "1.0.0",
    "description": "ExpressJS backend with TypeScript, MongoDB, JWT authentication, and Stripe integration",
    "main": "dist/server.js",
    "scripts": {
        "start": "node dist/server.js",
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "build": "tsc",
        "test": "jest",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix"
    },
    "keywords": [
        "express",
        "typescript",
        "mongodb",
        "jwt",
        "stripe",
        "authentication"
